/api/recipes\", newRecipe, { headers: { Authorization: `Bearer ${storedToken}` } })\n    .then(res => res.data)\n    .catch(errorHandler);\n};\n \nexport default {\n  getRecipes,\n  uploadImage,\n  createRecipe,\n};","/Users/jessica/Desktop/Portfolio/Dyner/client/src/context/auth.context.js",[],[],"/Users/jessica/Desktop/Portfolio/Dyner/client/src/components/MobileNav.jsx",[],[],"/Users/jessica/Desktop/Portfolio/Dyner/client/src/components/Hamburger.jsx",[],[],"/Users/jessica/Desktop/Portfolio/Dyner/client/src/components/DesktopNav.jsx",[],[],{"ruleId":"139","severity":1,"message":"140","line":49,"column":6,"nodeType":"141","endLine":49,"endColumn":8,"suggestions":"142"},{"ruleId":"139","severity":1,"message":"143","line":69,"column":6,"nodeType":"141","endLine":69,"endColumn":13,"suggestions":"144"},{"ruleId":"145","severity":1,"message":"146","line":16,"column":10,"nodeType":"147","messageId":"148","endLine":16,"endColumn":17},{"ruleId":"145","severity":1,"message":"149","line":16,"column":19,"nodeType":"147","messageId":"148","endLine":16,"endColumn":29},{"ruleId":"139","severity":1,"message":"150","line":38,"column":6,"nodeType":"141","endLine":38,"endColumn":8,"suggestions":"151"},{"ruleId":"139","severity":1,"message":"152","line":47,"column":6,"nodeType":"141","endLine":47,"endColumn":14,"suggestions":"153"},{"ruleId":"145","severity":1,"message":"154","line":8,"column":8,"nodeType":"147","messageId":"148","endLine":8,"endColumn":19},{"ruleId":"139","severity":1,"message":"155","line":43,"column":6,"nodeType":"141","endLine":43,"endColumn":8,"suggestions":"156"},{"ruleId":"139","severity":1,"message":"152","line":56,"column":6,"nodeType":"141","endLine":56,"endColumn":14,"suggestions":"157"},{"ruleId":"139","severity":1,"message":"158","line":31,"column":6,"nodeType":"141","endLine":31,"endColumn":8,"suggestions":"159"},{"ruleId":"139","severity":1,"message":"152","line":74,"column":6,"nodeType":"141","endLine":74,"endColumn":23,"suggestions":"160"},{"ruleId"